{
	"name": "faster-javascript",
	"displayName": "faster-javascript",
	"description": "Faster coding in JavaScript, TypeScript, React, and C/C++ with custom VS Code snippets.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.103.0"
	},
	"categories": [
		"Snippets"
	],
	"contributes": {
		"snippets": [
			{
				"language": "javascript",
				"path": "./snippets/js/javascript.code-snippets"
			},
			{
				"language": "javascriptreact",
				"path": "./snippets/js/javascript.code-snippets"
			},
			{
				"language": "javascriptreact",
				"path": "./snippets/js/javascript-react.code-snippets"
			},
			{
				"language": "typescript",
				"path": "./snippets/ts/typescript.code-snippets"
			},
			{
				"language": "typescriptreact",
				"path": "./snippets/ts/typescript.code-snippets"
			},
			{
				"language": "typescriptreact",
				"path": "./snippets/ts/typescript-react.code-snippets"
			},
			{
				"language": "cpp",
				"path": "./snippets/cp/cpp.code-snippets"
			},
			{
				"language": "c",
				"path": "./snippets/cp/c.code-snippets"
			}
		]
	},
	"publisher": "ldanh270",
	"license": "MIT"
}
